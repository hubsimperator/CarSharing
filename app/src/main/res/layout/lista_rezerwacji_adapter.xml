<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rezerwacja_layout"
    android:weightSum="4"
    >


    <TextView
        android:id="@+id/subject_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textStyle="bold"
        android:textSize="20dp"
        android:gravity="center"
        android:layout_marginTop="15dp"
        android:text="Temat rezerwacji" />

    <TextView
        android:id="@+id/startdate_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textSize="18dp"
        android:layout_marginTop="10dp"

        android:layout_marginLeft="20dp"

        android:text="Data poczatkowa" />

    <TextView
        android:id="@+id/enddate_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:layout_weight="1"
        android:layout_marginLeft="20dp"

        android:text="Data koÅ„cowa" />

    <TextView
        android:id="@+id/carname_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:textSize="18dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="20dp"
        android:text="Samochod wybrany np tojota" />
</LinearLayout>
